{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "PostsDatabase": "Host=localhost;Port=6432;Database=posts;Username=postgres;Password=postgres"
  },
  "JwtOptions": {
    "Issuer": "PostsApi",
    "Audience": "PostsFrontend",
    "Key": "PUT_32+_SYMBOLS_RANDOM_KEY_HERE",
    "ExpiresMinutes": 60
  },
  "EncryptionOptions": {
    "EncryptionKey": "PUT_32+_SYMBOLS_RANDOM_KEY_HERE________________"
  },
  "S3Options": {
    "ServiceUrl": "http://localhost:9000",
    "AccessKey": "minioadmin",
    "SecretKey": "minioadmin",
    "BucketName": "my-bucket",
    "ForcePathStyle": true,
    "PublicDomain": "http://localhost:9000/my-bucket"
  },
  "RunMigrations": true,
  "S3ConfigureCleanup": true
}
